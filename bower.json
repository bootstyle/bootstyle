{
  "name": "bootstyle",
  "version": "0.0.1",
  "homepage": "www.usebootstyle.com",
  "authors": [
    "Levi Thomason <me@levithomason.com>"
  ],
  "description": "Twitter Boostrap customizer for developers.",
  "main": "index.html",
  "keywords": [
    "Bootstrap",
    "Bootstrap Customizer",
    "Bootstrap Theme",
    "Live Reload",
    "LESS"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-spectrum-colorpicker": "~1.0.13",
    "bootstrap": "~3.2.0",
    "jquery": "~2.1.3",
    "angularjs": "~1.3.8",
    "angular-route": "~1.3.8",
    "angular-sanitize": "~1.3.8",
    "spectrum": "~1.6.0",
    "tinycolor": "~1.1.1",
    "FileSaver": "*",
    "codemirror": "~4.9.0"
  },
  "devDependencies": {},
  "overrides": {
    "bootstrap": {
      "main": [
        "./dist/fonts/glyphicons-halflings-regular.eot",
        "./dist/fonts/glyphicons-halflings-regular.svg",
        "./dist/fonts/glyphicons-halflings-regular.ttf",
        "./dist/fonts/glyphicons-halflings-regular.woff",
        "./less/**/*.*"
      ]
    }
  },
  "resolutions": {
    "spectrum": "1.4.1",
    "angular": "~1.2.8"
  }
}
