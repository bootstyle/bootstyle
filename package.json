{
  "name": "Bootstyle",
  "version": "0.2.0",
  "description": "Boostrap customizer for developers.",
  "repository": "greenkahuna/bootstyle",
  "author": "Levi Thomason <me@levithomason.com> (http://levithomason.com)",
  "contributors": [],
  "dependencies": {
    "color": "^0.6.0"
  },
  "devDependencies": {
    "browserify": "^4.1.11",
    "browserify-shim": "^3.6.0",
    "connect-livereload": "^0.4.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-uglify": "^0.5.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-contrib-connect": "^0.8.0",
    "grunt-contrib-less": "^0.11.3",
    "grunt-bowercopy": "^1.1.0",
    "grunt-bower-install-simple": "^0.9.3",
    "grunt-browserify": "^2.1.1",
    "grunt-npm-install": "^0.1.0",
    "load-grunt-tasks": "^0.6.0",
    "protractor": "^0.24.2",
    "watchify": "^0.10.2"
  },
  "engines": {
    "node": "0.10.x"
  },
  "main": "dist/js/app.js",
  "browser": {
    "angular": "./app/js/bower/angular/angular.js",
    "angular_sanitize": "./app/js/bower/angular-sanitize/angular-sanitize.js",
    "angular_spectrum_colorpicker": "./app/js/bower/angular-spectrum-colorpicker/angular-spectrum-colorpicker.js",
    "bootstrap": "./app/js/bower/bootstrap/bootstrap.js",
    "codemirror": "./app/js/bower/codemirror/codemirror.js",
    "codemirror_mode_css": "./app/js/bower/codemirror-mode-css/codemirror-mode-css.js",
    "codemirror_mode_htmlmixed": "./app/js/bower/codemirror-mode-htmlmixed/codemirror-mode-htmlmixed.js",
    "codemirror_mode_javascript": "./app/js/bower/codemirror-mode-javascript/codemirror-mode-javascript.js",
    "codemirror_mode_xml": "./app/js/bower/codemirror-mode-xml/codemirror-mode-xml.js",
    "FileSaver": "./app/js/bower/FileSaver/FileSaver.js",
    "jquery": "./app/js/bower/jquery/jquery.js",
    "modernizr": "./app/js/bower/modernizr/modernizr.js",
    "spectrum": "./app/js/bower/spectrum/spectrum.js",
    "tinycolor": "./app/js/bower/tinycolor/tinycolor.js",
    "tinycolor_v1api": "./app/js/vendor/tinycolor-v1api.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
          "jquery:$"
      ]
    },
    "angular_spectrum_colorpicker": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular_sanitize": {
      "depends": [
        "angular:angular"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    },
    "codemirror": {
      "exports": "codemirror"
    },
    "codemirror_mode_css": {
      "exports": "codemirror_mode_css"
    },
    "codemirror_mode_htmlmixed": {
      "exports": "codemirror_mode_htmlmixed"
    },
    "codemirror_mode_javascript": {
      "exports": "codemirror_mode_javascript"
    },
    "codemirror_mode_xml": {
      "exports": "codemirror_mode_xml"
    },
    "FileSaver": {
      "exports": "FileSaver"
    },
    "jquery": {
      "exports": "$"
    },
    "less": {
      "exports": "global:less"
    },
    "modernizr": {
      "exports": "modernizr"
    },
    "spectrum": {
      "depends": [
        "jquery:$"
      ]
    },
    "tinycolor": {
      "exports": "tinycolor",
      "depends": [
        "jquery:$"
      ]
    },
    "tinycolor_v1api": {
      "exports": "tinycolor_v1api",
      "depends": [
        "jquery:$"
      ]
    },
    "webfont": {
      "exports": "global:WebFont"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "postinstall": "$(npm bin)/webdriver-manager update",
    "test": "$(npm bin)/protractor test/protractor.conf.js",
    "setup": "npm install && bower install && grunt build && grunt serve:dev",
    "browserify_diagnostic": "BROWSERIFYSHIM_DIAGNOSTICS=1 browserify -d -e app/js/app.js -o build/js/app.js"
  }
}
