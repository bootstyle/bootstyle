{
  "name": "Bootstyle",
  "version": "0.2.0",
  "description": "Boostrap customizer for developers.",
  "repository": "greenkahuna/bootstyle",
  "author": "Levi Thomason <me@levithomason.com> (http://levithomason.com)",
  "contributors": [],
  "dependencies": {
    "color": "^0.6.0"
  },
  "devDependencies": {
    "browserify": "^4.1.11",
    "browserify-shim": "^3.6.0",
    "protractor": "^0.24.2",
    "watchify": "^0.10.2",
    "gulp": "^3.8.5",
    "gulp-flatten": "0.0.2",
    "gulp-bower-files": "~0.2.5",
    "gulp-filter": "~0.5.0",
    "vinyl-source-stream": "~0.1.1",
    "run-sequence": "~0.3.6",
    "merge-stream": "~0.1.2",
    "gulp-minify-css": "~0.3.6",
    "gulp-concat": "~2.2.0",
    "gulp-uglify": "~0.3.1",
    "gulp-util": "~2.2.19",
    "del": "~0.1.1",
    "gulp-watch": "~0.6.8",
    "gulp-plumber": "~0.6.3",
    "gulp-streamify": "0.0.5",
    "gulp-webserver": "~0.3.4",
    "gulp-open": "~0.2.8"
  },
  "engines": {
    "node": "0.10.x"
  },
  "main": "./app/js/app.js",
  "browser": {
    "angular": "./app/js/bower/angular.js",
    "angular_sanitize": "./app/js/bower/angular-sanitize.js",
    "angular_touch": "./app/js/bower/angular-touch.js",
    "angular_carousel": "./app/js/bower/angular-carousel.js",
    "angular_spectrum_colorpicker": "./app/js/bower/angular-spectrum-colorpicker.js",
    "bootstrap": "./app/js/bower/bootstrap.js",
    "codemirror": "./app/js/bower/codemirror.js",
    "codemirror_mode_css": "./app/js/bower/css.js",
    "codemirror_mode_htmlmixed": "./app/js/bower/htmlmixed.js",
    "codemirror_mode_javascript": "./app/js/bower/javascript.js",
    "codemirror_mode_xml": "./app/js/bower/xml.js",
    "FileSaver": "./app/js/bower/FileSaver.js",
    "jquery": "./app/js/bower/jquery.js",
    "modernizr": "./app/js/bower/modernizr.js",
    "spectrum": "./app/js/bower/spectrum.js",
    "tinycolor": "./app/js/bower/tinycolor.js",
    "tinycolor_v1api": "./app/js/vendor/tinycolor-v1api.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery:$"
      ]
    },
    "angular_carousel": {
      "depends": [
        "angular:angular",
        "angular_touch:angular_touch"
      ]
    },
    "angular_sanitize": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular_spectrum_colorpicker": {
      "depends": [
        "angular:angular"
      ]
    },
    "angular_touch": {
      "depends": [
        "angular:angular"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:$"
      ]
    },
    "codemirror": {
      "exports": "codemirror"
    },
    "codemirror_mode_css": {
      "exports": "codemirror_mode_css"
    },
    "codemirror_mode_htmlmixed": {
      "exports": "codemirror_mode_htmlmixed"
    },
    "codemirror_mode_javascript": {
      "exports": "codemirror_mode_javascript"
    },
    "codemirror_mode_xml": {
      "exports": "codemirror_mode_xml"
    },
    "FileSaver": {
      "exports": "FileSaver"
    },
    "jquery": {
      "exports": "$"
    },
    "less": {
      "exports": "global:less"
    },
    "modernizr": {
      "exports": "modernizr"
    },
    "spectrum": {
      "depends": [
        "jquery:$"
      ]
    },
    "tinycolor": {
      "exports": "tinycolor",
      "depends": [
        "jquery:$"
      ]
    },
    "tinycolor_v1api": {
      "exports": "tinycolor_v1api",
      "depends": [
        "jquery:$"
      ]
    },
    "webfont": {
      "exports": "global:WebFont"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "postinstall": "$(npm bin)/webdriver-manager update",
    "test": "$(npm bin)/protractor test/protractor.conf.js",
    "setup": "npm install && bower install && gulp",
    "browserify_diagnostic": "BROWSERIFYSHIM_DIAGNOSTICS=1 browserify -d -e app/js/app.js -o build/js/app.js"
  }
}
